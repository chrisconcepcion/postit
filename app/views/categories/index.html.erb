<%= render 'shared/header', header: "Categories:" %>
<div id="flash">
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, :id => "flash_#{name}" %>
  <% end %>
</div>
<ul>
<% @category.each do |category| %>
<div class="well">
	<%= link_to category.name, category %>
	</div>
<% end %>
</ul>